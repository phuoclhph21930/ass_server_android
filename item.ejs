<tr>
    <th scope="row"><%= i+1 %></th>
    <td><h2><%= product.tensp %></h2></td>
    <td><img style="width: 150px; height: 150px" src="<%= product.anhsp %>"></td>
    <td><h2>
        <%= product.mau %>
    </h2></td>
    <td><h2><%= product.loaisp %></h2></td>
    <td><h2><%= product.makh %></h2></td>
    <td><h2><%= product.tenkh %></h2></td>
 
    <td class="align-middle text-center">
        <a href="javascript:;" class="text-secondary font-weight-bold text-xs" data-id="<%= product._id %>" data-bs-toggle="modal" data-bs-target="#editProductModal">
            <img style="height: 50px;width: 50px;" src="data:image/png;base64," class="img-circle" alt="Cinque Terre">
        </a>
        
        <a href="javascript:;" class="btn_delete_product text-secondary font-weight-bold text-xs text-danger" data-id="<%= product._id %>" data-bs-toggle="modal" data-bs-target="#deleteProductModal">
            Delete
        </a>
    </td>
</tr>